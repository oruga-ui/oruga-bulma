{
  "name": "@oruga-ui/theme-bulma",
  "version": "0.2.11",
  "description": "Bulma theme for Oruga",
  "license": "MIT",
  "main": "dist/bulma.js",
  "module": "dist/bulma.mjs",
  "unpkg": "dist/bulma.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oruga-ui/theme-bulma.git"
  },
  "bugs": {
    "url": "https://github.com/oruga-ui/theme-bulma/issues"
  },
  "scripts": {
    "dev": "vite --force",
    "dev:lib": "rimraf dist && rollup -c --watch",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && rollup -c",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "pack:lib": "echo \"$PWD/$(npm pack)\""
  },
  "dependencies": {
    "bulma": "0.9.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "6.5.0",
    "@oruga-ui/oruga-next": "0.8.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "10.4.17",
    "clean-css-cli": "^5.6.2",
    "core-js": "3.35.1",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "npm-check-updates": "^16.14.14",
    "postcss": "8.4.34",
    "prettier": "^3.2.5",
    "rimraf": "5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-sass": "1.12.21",
    "sass": "1.70.0",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vue": "3.4.15",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.27"
  }
}
