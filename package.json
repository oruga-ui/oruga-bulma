{
  "name": "@oruga-ui/theme-bulma",
  "version": "0.2.11",
  "description": "Bulma theme for Oruga",
  "license": "MIT",
  "main": "dist/bulma.js",
  "module": "dist/bulma.mjs",
  "unpkg": "dist/bulma.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oruga-ui/theme-bulma.git"
  },
  "bugs": {
    "url": "https://github.com/oruga-ui/theme-bulma/issues"
  },
  "scripts": {
    "dev": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && rollup -c",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "bulma": "0.9.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "6.5.0",
    "@oruga-ui/oruga-next": "0.8.2",
    "@rollup/plugin-typescript": "11.1.5",
    "@vitejs/plugin-vue": "4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "10.4.16",
    "clean-css-cli": "^5.6.2",
    "core-js": "3.33.3",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "npm-check-updates": "^16.14.11",
    "postcss": "8.4.31",
    "prettier": "^3.1.0",
    "rimraf": "5.0.5",
    "rollup": "^4.6.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-sass": "1.12.21",
    "sass": "1.69.5",
    "tslib": "2.6.2",
    "typescript": "5.3.2",
    "vite": "^5.0.4",
    "vue": "3.3.9",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.24"
  }
}
